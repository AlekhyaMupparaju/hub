.container.newsfeeds
  - if (user.logged())
    .row
      article.col-xs-10.col-md-8.col-md-offset-2.col-xs-offset-1.newsfeed.start
        .profile.pull-left
          img(src=user.profilePictureUrl || user.gravatar)
        a.name.pull-left(href='#') #{user.firstName} #{user.lastName}
  - else
    .jumbotron.text-center
      h1=t('newsfeed.welcome.title')
      p!=t('newsfeed.welcome.body')
  .feeds
